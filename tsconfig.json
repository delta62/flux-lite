{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "lib": [ "es5", "es2015.promise" ],
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strictNullChecks": true
  }
}
