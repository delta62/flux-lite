{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "typeRoots": [ "node_modules/@types" ],
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strictNullChecks": true
  }
}
